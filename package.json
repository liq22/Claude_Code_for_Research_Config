{
  "name": "claude-code-research-config",
  "version": "1.0.0",
  "description": "Advanced multi-agent research configuration for Nature-level academic paper writing with automatic execution logging",
  "main": "index.js",
  "scripts": {
    "logs:view": "python scripts/logging/log_viewer.py",
    "logs:search": "python scripts/logging/log_analyzer.py search",
    "logs:analytics": "python scripts/logging/log_analyzer.py analytics",
    "logs:recent": "python scripts/logging/log_viewer.py --recent 10",
    "logs:cleanup": "python .claude/hooks/session-manager.py cleanup",
    "logs:status": "python .claude/hooks/session-manager.py status",
    "logs:monitor": "python .claude/hooks/session-manager.py monitor",
    "test:logging": "python scripts/logging/claude_logger.py",
    "test:keywords": "python scripts/logging/keywords_extractor.py",
    "test:analyzer": "python scripts/logging/log_analyzer.py analytics --days 1",
    "install:python-deps": "pip install -r requirements.txt",
    "setup:logging": "python -c \"from scripts.logging.claude_logger import get_claude_logger; print('âœ… Logging system ready')\"",
    "export:analytics": "python scripts/logging/log_analyzer.py analytics --export analytics_$(date +%Y%m%d).md --format markdown"
  },
  "keywords": [
    "claude-code",
    "research",
    "academic-writing",
    "ai-agents",
    "logging",
    "analytics",
    "multi-agent-system",
    "nature-level-papers"
  ],
  "author": "Claude Code Research System",
  "license": "MIT",
  "dependencies": {
    "ccusage": "^16.1.2"
  },
  "devDependencies": {
    "yaml": "^2.3.4",
    "chalk": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/user/claude-code-research-config.git"
  },
  "bugs": {
    "url": "https://github.com/user/claude-code-research-config/issues"
  },
  "homepage": "https://github.com/user/claude-code-research-config#readme"
}
